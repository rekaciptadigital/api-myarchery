{
    "name": "rcd/archery",
    "description": "Archery API.",
    "keywords": ["framework", "laravel", "lumen", "archery", "API"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "barryvdh/laravel-snappy": "*",
        "doctrine/dbal": "^2.13",
        "dompdf/dompdf": "^1.2",
        "flipbox/lumen-generator": "^8.2",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "illuminate/mail": "5.7.*",
        "illuminate/redis": "5.7.*",
        "laravel/lumen-framework": "5.7.*",
        "league/flysystem": " ~1.0",
        "maatwebsite/excel": "^3.1",
        "midtrans/midtrans-php": "^2.5",
        "mpdf/mpdf": "^8.0",
        "mpdf/qrcode": "^1.1",
        "orumad/lumen-config-cache": "*",
        "ramsey/collection": "1.1.4",
        "ramsey/uuid": "^4.2",
        "sulfanoaf/api-util": "^1.0@alpha",
        "tibonilab/pdf-lumen-bundle": "~2.0.0",
        "tymon/jwt-auth": "^1.0",
        "vlucas/phpdotenv": "~2.2",
        "wemersonjanuario/laravelpdf": "1.1.*",
        "wemersonjanuario/wkhtmltopdf-windows": "0.12.2.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
